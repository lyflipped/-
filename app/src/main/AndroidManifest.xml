<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liyang.getshbayimage">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/a10"
        android:label="@string/app_name"
        android:logo="@drawable/wechat_24px"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ToutiaoActivity"
            android:logo="@drawable/back"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".wechatdemo"
            android:theme="@style/Theme.Myactionbar"
            android:icon="@drawable/wechat_24px"
            android:label="@string/weixin"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>